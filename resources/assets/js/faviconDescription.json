{
  "masterPicture": "resources/assets/img/logo.png",
  "iconsPath": "/build/favicon/",
  "design": {
    "ios": {
      "pictureAspect": "backgroundAndMargin",
      "backgroundColor": "#ffffff",
      "margin": "11%",
      "assets": {
        "ios6AndPriorIcons": true,
        "ios7AndLaterIcons": true,
        "precomposedIcons": true,
        "declareOnlyDefaultIcon": true
      }
    },
    "desktopBrowser": {},
    "windows": {
      "pictureAspect": "whiteSilhouette",
      "backgroundColor": "#cf74b9",
      "onConflict": "override",
      "assets": {
        "windows80Ie10Tile": true,
        "windows10Ie11EdgeTiles": {
          "small": true,
          "medium": true,
          "big": true,
          "rectangle": true
        }
      }
    },
    "androidChrome": {
      "pictureAspect": "noChange",
      "themeColor": "#ffffff",
      "manifest": {
        "name": "Creativados",
        "display": "standalone",
        "orientation": "notSet",
        "onConflict": "override",
        "declared": true
      },
      "assets": {
        "legacyIcon": true,
        "lowResolutionIcons": true
      }
    },
    "safariPinnedTab": {
      "pictureAspect": "silhouette",
      "themeColor": "#cf74b9"
    }
  },
  "settings": {
    "compression": 5,
    "scalingAlgorithm": "Mitchell",
    "errorOnImageTooSmall": false,
    "readmeFile": false,
    "htmlCodeFile": true,
    "usePathAsIs": false
  }
}