{% include "required.twig" %}
<!-- Twitter Card data -->
<meta name="twitter:card" value="{{ seo_info.twitter_summary|trans }}">

<!-- Open Graph data -->
<meta property="og:title" content="{{ seo_info.title_share|trans }}"/>
<meta property="og:type" content="article"/>
<meta property="og:url" content="{{ seo_info.url_share }}"/>
<meta property="og:image" content="{{ seo_info.image_share }}"/>
<meta property="og:description" content="{{ seo_info.description_share|trans }}"/>