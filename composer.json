{
  "name": "dtkahl/slim-skeleton",
  "description": "A Slim Framework skeleton application for rapid development",
  "keywords": [
    "microframework",
    "rest",
    "router",
    "psr7",
    "slim3",
    "skeleton"
  ],
  "homepage": "http://github.com/dtkahl/Slim-Skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Kahl",
      "email": "kontakt@daniel-kahl.de"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "slim/slim": "^3.1",
    "monolog/monolog": "^1.17",
    "slim/twig-view": "^2.2",
    "vlucas/phpdotenv": "^2.4",
    "dtkahl/php-simple-config": "^1.1",
    "mnapoli/silly": "1.5.*",
    "mnapoli/silly-php-di": "^1.1",
    "php-di/slim-bridge": "^1.0",
    "dtkahl/php-file-cache": "^1.3",
    "slim/flash": "^0.1.0",
    "phpmailer/phpmailer": "~5.2",
    "twig/extensions": "^1.4.1",
    "bryanjhv/slim-session": "~3.0",
    "gettext/gettext": "^4.1",
    "nochso/html-compress-twig": "^2.0",
    "erusev/parsedown": "^1.6"
  },
  "require-dev": {
    "php-console/php-console": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "process-timeout": 0
  }
}
